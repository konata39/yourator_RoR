<p id="notice"><%= notice %></p>

<h1>Db Companies</h1>

<table>
  <thead>
    <tr>
      <th>Company name</th>
      <th>Company info</th>
      <th>Company target</th>
      <th>Company media</th>
      <th>Company salary</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @db_companies.each do |db_company| %>
      <tr>
        <td><%= db_company.company_name %></td>
        <td><%= db_company.company_info %></td>
        <td><%= db_company.company_target %></td>
        <td><%= db_company.company_media %></td>
        <td><%= db_company.company_salary %></td>
        <td><%= link_to 'Show', db_company %></td>
        <td><%= link_to 'Edit', edit_db_company_path(db_company) %></td>
        <td><%= link_to 'Destroy', db_company, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Db Company', new_db_company_path %>
